{
    "openapi": "3.0.0",
    "info": {
        "title": "Node Rentals Documentation",
        "description": "This is an API for Car Rentals",
        "version": "1.0.0",
        "contact": {
            "email": "g_machado1@hotmail.com"
        }
    },
    "paths": {
        "/categories": {
            "post": {
                "tags": [ "Category" ],
                "summary": "Create a category",
                "description": "Create a new category",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "name": "Category name sample",
                                    "description": "Category description sample"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Category already exists"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            },
            "get": {
                "tags": [ "Category" ],
                "summary": "List all categories",
                "description": "List all categories",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "name": {
                                                "type": "string"
                                            },
                                            "description": {
                                                "type": "string"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "/categories/import": {
            "post": {
                "tags": [ "Category" ],
                "summary": "Import some categories",
                "description": "Import categories from a csv file",
                "requestBody": {
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "file": {
                                        "type": "string",
                                        "format": "binary"
                                    }
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Imported"
                    },
                    "400": {
                        "description": "File missing"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        },
        "/specifications": {
            "post": {
                "tags": [ "Specification" ],
                "summary": "Create a specification",
                "description": "Create a new specification",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string"
                                    },
                                    "description": {
                                        "type": "string"
                                    }
                                },
                                "example": {
                                    "name": "Specification name sample",
                                    "description": "Specification description sample"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Created"
                    },
                    "400": {
                        "description": "Specification already exists"
                    },
                    "500": {
                        "description": "Internal server error"
                    }
                }
            }
        }
    }
}